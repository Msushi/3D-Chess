public void paint(Graphics g){
	for(ChessPiece[][] a : ChessGame.board){
		for(ChessPiece[] b : a){
			for(ChessPiece c : b){
				if(!c.getIcon().equals(null)){
					int[] pos = c.getPosition();
					if(pos[2] == elevatorLevel){
						c.getIcon().paintIcon(boardThing, g, pos[0]*45 + 10, pos[1]*45);
					}
				}
			}
		}
	}
}
